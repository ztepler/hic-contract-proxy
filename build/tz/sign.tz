{ parameter
    (or (or (pair %is_signed (pair (contract %callback bool) (nat %id)) (address %participant))
            (nat %sign))
        (nat %unsign)) ;
  storage (big_map (pair address nat) unit) ;
  code { LAMBDA
           unit
           unit
           { DROP ;
             PUSH mutez 0 ;
             AMOUNT ;
             COMPARE ;
             EQ ;
             IF { UNIT }
                { PUSH string "This entrypoint should not receive tez" ; FAILWITH } } ;
         SWAP ;
         UNPAIR ;
         IF_LEFT
           { IF_LEFT
               { SWAP ;
                 UNIT ;
                 DIG 3 ;
                 SWAP ;
                 EXEC ;
                 DROP ;
                 SWAP ;
                 DUP ;
                 DUG 2 ;
                 CAR ;
                 CAR ;
                 PUSH mutez 0 ;
                 DUP 3 ;
                 DUP 5 ;
                 CAR ;
                 CDR ;
                 DIG 5 ;
                 CDR ;
                 PAIR ;
                 GET ;
                 IF_NONE { PUSH bool False } { DROP ; PUSH bool True } ;
                 TRANSFER_TOKENS ;
                 SWAP ;
                 NIL operation ;
                 DIG 2 ;
                 CONS ;
                 PAIR }
               { SWAP ;
                 UNIT ;
                 DIG 3 ;
                 SWAP ;
                 EXEC ;
                 DROP ;
                 SWAP ;
                 SENDER ;
                 PAIR ;
                 SWAP ;
                 UNIT ;
                 DIG 2 ;
                 SWAP ;
                 SOME ;
                 SWAP ;
                 UPDATE ;
                 NIL operation ;
                 PAIR } }
           { SWAP ;
             UNIT ;
             DIG 3 ;
             SWAP ;
             EXEC ;
             DROP ;
             SWAP ;
             SENDER ;
             PAIR ;
             NONE unit ;
             SWAP ;
             UPDATE ;
             NIL operation ;
             PAIR } } }

